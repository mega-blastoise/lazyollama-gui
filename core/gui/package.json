{
  "name": "@lazyollama-gui/browser-gui",
  "type": "module",
  "packageManager": "bun@1.2.3",
  "version": "0.0.1-alpha.0.1",
  "private": true,
  "engines": {
    "bun": ">=1.2.3"
  },
  "main": "./out/index.js",
  "scripts": {
    "web:bundle": "DEBUG=* NODE_ENV=production bun run build/web.ts",
    "web:patch-bundled-css": "cp out/gui/browser/hydrate.*.css out/css/lazyollama-variables.css && mv out/index.css out/css/lazyollama-styles.css",
    "web:patch-public-styles": "cp public/styles.css out/css/lazyollama-base.css",
    "web:mkdir-css": "mkdir -p out/css",
    "web:rm-css": "rm out/gui/browser/hydrate.*.css",
    "web:build": "run-s web:bundle web:mkdir-css web:patch-bundled-css web:patch-public-styles web:rm-css",
    "server:build": "DEBUG=* NODE_ENV=production bun run build/server.ts",
    "clean": "rm -rf out",
    "build": "run-p server:build web:build",
    "dev": "DEBUG=* NODE_ENV=development bun run --watch src/index.ts",
    "start": "DEBUG=* NODE_ENV=production bun run out/index.js",
    "docker": "bun start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@lazyollama-gui/typescript-common-types": "workspace:*",
    "@lazyollama-gui/typescript-configs": "workspace:*",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/addon-webpack5-compiler-swc": "^2.1.0",
    "@storybook/blocks": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-webpack5": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@types/bun": "latest",
    "@types/debug": "^4.1.12",
    "@types/lodash.isequal": "^4.5.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "npm-run-all": "^4.1.5",
    "storybook": "^8.6.4"
  },
  "dependencies": {
    "@lazyollama-gui/typescript-clients": "workspace:*",
    "@lazyollama-gui/typescript-common": "workspace:*",
    "@lazyollama-gui/typescript-common-types": "workspace:*",
    "@lazyollama-gui/typescript-react-components": "workspace:*",
    "@tanstack/react-query": "^5.67.2",
    "chalk": "^5.4.1",
    "classnames": "^2.5.1",
    "debug": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lucide-react": "^0.479.0",
    "node-emoji": "^2.2.0",
    "node-schedule": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "sleepydogs": "^1.0.8-alpha.4",
    "web-vitals": "^4.2.4",
    "zustand": "^5.0.3"
  }
}
